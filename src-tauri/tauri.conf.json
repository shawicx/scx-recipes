{
  "productName": "SmartDiet",
  "version": "0.1.0",
  "identifier": "com.example.smartdiet",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:2025",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Smart Diet",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://assetcdn.localhost; media-src 'self' asset: https://assetcdn.localhost; style-src 'self' 'unsafe-inline'; script-src 'self'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
